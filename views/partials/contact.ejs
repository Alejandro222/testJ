<!-- Contact Section -->
<section id="contact" name="contact">
  <div>
    <!-- class="container" -->
    <div class="row">
      <div class="col-md-12 text-center">
        <h2 class="section-heading">Contáctenos </h2>
        <h4 class=" section-heading">Estamos para Servirle!</h4>
      </div>
    </div>
    <div class="contactStyle">


      <!-- otra parte -->
      <div class="col-md-6">

        <div class="">
          <h2 class="text-center">JARSOL</h2>
          <br>
          <div class="row">
            <div class="row">
              <div class="text-center  col-md-4 col-xs-12 col-lg-5">
                <i class="glyphicon glyphicon-phone-alt fa-stack-1x fa-inverse fa-left iconSize"></i>
              </div>
            </div>
            <div class="row">
              <div class="col-md-8 col-xs-12 col-lg-12">
                <h3 class="contactStyle">Teléfono: </h3>
                <ul class="styleList list-unstyled">
                  <li class="">
                    <p class="styleDireccion">Oficina: (506) 2474 9898</p>

                  </li>
                  <li class="">
                    <p class="styleDireccion">Celular: (506) 8817 0020</p>

                  </li>
                </ul>
              </div>

            </div>

          </div>

          <br>
          <div class="row">
            <div class="row">
              <div class="text-center  col-md-4 col-xs-12 col-lg-5">
                <i class="fa fa-map-marker fa-stack-1x fa-inverse fa-left iconSize"></i>
              </div>
            </div>

            <div class="row">
              <div class="col-md-8 col-xs-12 col-lg-12">
                <h3 class="contactStyle">Dirección: </h3>
                <ul class="styleList list-unstyled">
                  <li class="">
                    <!-- <h5>50 mts Sur Baterías del Norte Aguas Zarcas, San Carlos</h5> -->
                    <p class="styleDireccion">50 mts Sur Baterías del Norte Aguas Zarcas, San Carlos</p>
                  </li>
                </ul>
              </div>
            </div>

          </div>

          <br>
          <div class="row">
          <div class="row">
           <div class="text-center  col-md-4 col-sm-4 col-lg-5">
              <i class="glyphicon glyphicon-time fa-stack-1x fa-inverse fa-left  iconSize"></i>
            </div>
          </div>

           <div class="row">
            <div class="col-md-8 col-sm-8 col-lg-12">
              <h3 class="contactStyle">Horario </h3>
              <!-- <h5 class="contactStyle">Atención: </h5> -->
              <ul class="styleList list-unstyled">
                <li class="">
                  <p class="styleDireccion">Lunes - Viernes 7:00 am a 5:00 pm</p>
                  <p class="styleDireccion">Sábado y Domingo Cerrado</p>

                </li>

              </ul>
            </div>
           </div>


          </div>
          <br>
        </div>
      </div>
      <div class="row ">
        <div class="marginStyle ">

          <div class="col-md-6">
            <form name="sentMessage" id="contactForm" novalidate>
              <div class="row">
                <div class="col-md-6">
                  <div class="form-group">

                    <input type="text" class="form-control" placeholder="Nombre Completo *" id="name" required data-validation-required-message="Please enter your name.">
                    <p class="help-block text-danger"></p>
                  </div>
                  <div class="form-group">
                    <input type="email" class="form-control" placeholder="Correo Electrónico *" id="email" required data-validation-required-message="Please enter your email address.">
                    <p class="help-block text-danger"></p>
                  </div>
                  <div class="form-group">
                    <input type="tel" class="form-control" placeholder="Teléfono *" id="phone" required data-validation-required-message="Please enter your phone number.">
                    <p class="help-block text-danger"></p>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="form-group">
                    <textarea class="form-control" placeholder="Mensaje *" id="message" required data-validation-required-message="Please enter a message."></textarea>
                    <p class="help-block text-danger"></p>
                  </div>
                </div>
                <div class="clearfix"></div>
                <div class="col-md-12 text-center">
                  <div id="success"></div>
                  <button type="submit" class="btn btn-xl">Enviar Mensaje</button>
                </div>
              </div>
            </form>
          </div>
        </div>


      </div>

    </div>

  </div>
</section>
